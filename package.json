{
    "name": "vue-dk-toast",
    "description": "Lightweight toast notification plugin for Vue 3",
    "version": "1.1.2",
    "private": false,
    "main": "/dist/dkToast.umd.min.js",
    "scripts": {
        "serve": "vue-cli-service serve",
        "lint": "vue-cli-service lint",
        "lib": "vue-cli-service build --target lib --name dkToast ./src/toast.js"
    },
    "dependencies": {
        "core-js": "^3.6.5"
    },
    "peerDependencies": {
        "vue": "^3.0.0-0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0-0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0-0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "files": [
        "dist/dkToast.umd.min.js"
    ],
    "externals": {
        "vue": "vue"
    },
    "keywords": [
        "vue",
        "vue3",
        "vuejs",
        "plugin",
        "toast",
        "notification",
        "notifications"
    ]
}
